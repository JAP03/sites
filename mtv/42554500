	//Scope 1st-party uuid to persist.
	var uuid_cookie_date = new Date("December 31, 2038");

	var uuid_cookie = "vmn_uuid=f3f44fb1-4c80-efc0-0123-926c15ca82a2;path=/;";
	var guid_cookie = "mtvn_guid=f3f44fb1-4c80-efc0-0123-926c15ca82a2;path=/;";
	if(uuid_cookie_date){
		uuid_cookie += "expires="+uuid_cookie_date.toGMTString();
		guid_cookie += "expires="+uuid_cookie_date.toGMTString();
	}
	document.cookie = uuid_cookie;
	document.cookie = guid_cookie;


