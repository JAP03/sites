$(document).ready(function() {

// jwc edgov-294
// hide BLOG, PR, SPEECHES, MEDIA ADVISORIES
  $("#block-views-HomePage_News_Views-block_2").hide();
  $("#block-views-HomePage_News_Views-block_3").hide();
  $("#block-views-HomePage_News_Views-block_4").hide();
  $("#block-views-HomePage_News_Views-block_5").hide();
  $(".news_tab").removeClass("active");
  $("#all-button").addClass("active");

  $("#all-button").click(function() {
    $("#block-views-HomePage_News_Views-block_1").show();
    $("#block-views-HomePage_News_Views-block_2").hide();
    $("#block-views-HomePage_News_Views-block_3").hide();
    $("#block-views-HomePage_News_Views-block_4").hide();
    $("#block-views-HomePage_News_Views-block_5").hide();
    $(".news_tab").removeClass("active");
    $("#all-button").addClass("active");
    return false;
  })

  $("#blog-button").click(function() {
    $("#block-views-HomePage_News_Views-block_1").hide();
    $("#block-views-HomePage_News_Views-block_2").show();
    $("#block-views-HomePage_News_Views-block_3").hide();
    $("#block-views-HomePage_News_Views-block_4").hide();
    $("#block-views-HomePage_News_Views-block_5").hide();
    $(".news_tab").removeClass("active");
    $("#blog-button").addClass("active");
    return false;
  })

  $("#press-releases-button").click(function() {
    $("#block-views-HomePage_News_Views-block_1").hide();
    $("#block-views-HomePage_News_Views-block_2").hide();
    $("#block-views-HomePage_News_Views-block_3").show();
    $("#block-views-HomePage_News_Views-block_4").hide();
    $("#block-views-HomePage_News_Views-block_5").hide();
    $(".news_tab").removeClass("active");
    $("#press-releases-button").addClass("active");
    return false;
  })

  $("#speeches-button").click(function() {
    $("#block-views-HomePage_News_Views-block_1").hide();
    $("#block-views-HomePage_News_Views-block_2").hide();
    $("#block-views-HomePage_News_Views-block_3").hide();
    $("#block-views-HomePage_News_Views-block_4").show();
    $("#block-views-HomePage_News_Views-block_5").hide();
    $(".news_tab").removeClass("active");
    $("#speeches-button").addClass("active");
    return false;
  })

  $("#media-advisories-button").click(function() {
    $("#block-views-HomePage_News_Views-block_1").hide();
    $("#block-views-HomePage_News_Views-block_2").hide();
    $("#block-views-HomePage_News_Views-block_3").hide();
    $("#block-views-HomePage_News_Views-block_4").hide();
    $("#block-views-HomePage_News_Views-block_5").show();
    $(".news_tab").removeClass("active");
    $("#media-advisories-button").addClass("active");
    return false;
  })

});
