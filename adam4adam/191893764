if(typeof localStorage !== 'undefined')
{
  //extending storage object so it can store objects as well
  Storage.prototype.setObject = function(key, value) {
      this.setItem(key, JSON.stringify(value));
  };

  Storage.prototype.getObject = function(key) {
	  if(this.getItem(key)!=null)
        return JSON.parse(this.getItem(key));
  };

  if(typeof localStorage.getObject('config') == 'undefined')
  {
	  var local_config = {
		  user_list_order:'date_last_login'
	  };
	  localStorage.setObject('config', local_config);
  }
  else
	  var local_config = localStorage.getObject('config');

  function send_local_settings(){
	  var config = localStorage.getObject('config');
	  return JSON.stringify(config);
  }

  function update_local_settings(key, value)
  {
	  local_config[key]=value;
	  localStorage.setObject('config', local_config);
  }
}
else
{
  function send_local_settings(){
    return '';
  }

  function update_local_settings(key, value)
  {
    return '';
  }
}