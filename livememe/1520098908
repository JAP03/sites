   	 if (cbtn == undefined) {
     	 var winder = document.getElementById('datderewinder');
        cbtn = document.createElement('div');
        winder.appendChild(cbtn);
        cbtn.style.position = 'absolute';
        cbtn.style.left = '766px';
        cbtn.style.top = '8px';
        cbtn.style.background = 'url("http://static.lvme.me/close.png")';
        cbtn.style.width = '31px';
        cbtn.style.height = '31px';
        cbtn.style.cursor = 'pointer';
        cbo.mo = false;
        cbo.md = false;
        cbo.dis = false;

        cbtn.onmouseover = function () {
         if (!cbo.dis) {
          cbo.mo = true;
          this.style.backgroundPosition = '0px -31px';
         }
        }
        cbtn.onmouseout = function () {
         if (!cbo.dis) {
          cbo.mo = false; 
          this.style.backgroundPosition = '0px 0px';
         }
        }
        cbtn.onmousedown = function () {
         if (!cbo.dis) {
          cbo.md = true;
          this.style.backgroundPosition = '0px -62px';
         }
        }
        cbtn.onmouseup = function () {
         if (!cbo.dis) {
          cbo.md = false;
          this.style.backgroundPosition = '0px 0px';
         }
        }
        cbtn.onclick = function () {
         var winder = document.getElementById('datderewinder');
         winder.style.visibility = 'hidden';
         var wh = document.getElementById('winderholder');
         wh.style.display = 'none';
		 var gbg = document.getElementById('greybg');
		 gbg.style.display = 'none';
         //start_mdh = 0;
         smwfr = 0;
         advanimexto.last_generated_captioner.onclick(); // like clicking "cancel" which used to say "add captions"
         return false;
        }
      }

   	 advanimexto.extraInputCallback = function () {
       var winder = document.getElementById('datderewinder');
       var wh = document.getElementById('winderholder');
       winder.style.visibility = 'visible';
       smw_iv = setInterval('showMemeWin()', 33);

       
       wofx = advanimexto.nw-450;
       winder.style.width = (780 + wofx)+'px';
       cbtn.style.left = (766 + wofx)+'px';
/*
   	  if (spi_iv == -1) {
   	   spi_iv = setInterval('spinna()', 33);
 	    }
*/
   	  spsh = false;
 	   }
      var wh = document.getElementById('winderholder');
      wh.style.display = 'inline';
	  var gbg = document.getElementById('greybg');
	  gbg.style.display = 'inline';
      wh.style.opacity = 0;
	  gbg.style.opacity = 0;
	  gbgfr = 0;
   	  if (gbg_iv == -1) {
   	   gbg_iv = setInterval('gbgfi()', 33);
 	    }

      advanimexto.submit_to_reddit = false;
      advanimexto.hide_loadie = true;
      advanimexto.last_generated_captioner.onclick();
      //btn.style.display = 'none';
 	   return false;
	  }
	  
	
	
	function clickedAdvAnimRadio (val) {
		if (advanimexto_radio_d.length > 0) {
			var dds = advanimexto_radio_d.split(", ");
			for (var qq = 0; qq<dds.length; qq++) {
				if (qq == val) {
					advanimexto_cur_radio = val;
					var cdds = dds[qq].split(":");
					smwfr = 0;
					advanimexto.preserve_captions = true;
					advanimexto.last_generated_captioner.onclick(); // like clicking "cancel" which used to say "add captions"
					advanimexto_target_url = "http://www.livememe.com/"+cdds[1];
					showEditor();
				}
			}
		}
	}
	  
	  
    
    
   var smwfr = 0;
   var smw_iv = '';
   var smfrs = 6;
   function showMemeWin () {
     smwfr++;
      if (smwfr >= smfrs) {
	    smwfr = smfrs;
	  }
      var winder = document.getElementById('datderewinder');
      var wh = document.getElementById('winderholder');
      var amt = (1 - Math.cos((smwfr/smfrs)*Math.PI))/2;
      wh.style.opacity = amt;
      //if (navigator.userAgent.toLowerCase().indexOf('chrome') == -1) { // Remarkably, chrome's 3d css actually renders rather poorly
       //winder.style['-webkit-transform'] = winder.style['-moz-transform'] = winder.style.MozTransform = 'perspective(1600px) rotateX('+(-Math.round(32 * (1 - amt) * 100)/100)+'deg)'; // Safari, FF, and Chrome
      //}
      if (smwfr >= smfrs) {
	    smwfr = smfrs;
        clearInterval(smw_iv);
      }
    }




		
