function base36bytesToInt (s) {
  var v = 0;
  for (var i = 0; i<s.length; i++) {
    var ch = s.charCodeAt(i);
    if (ch>=0x61 && ch<=0x7a) {
      v = (v*36) + ch + 10-0x61;
    } else if (ch>=0x41 && ch<=0x5a) {
      v = (v*36) + ch + 10-0x41; 
    } else if (ch>=0x30 && ch<=0x39) {
      v = (v*36) + ch - 0x30;
    }
  }
  return v;
}


function base36intToBytes (a) {
  if (a == 0) {
   return "0";
  }
  var s = "";
  var j = 8;
  var v;
  while (a > 0 && j > 0) {
   v = a%36;
   j--;
   if (v <= 9) {
    s = String.fromCharCode(0x30 + v) + s;
   } else {
    s = String.fromCharCode(0x61 + v - 10) + s;
   }
   a -= v;
   a /= 36;
  }
  return s;
}

function loadScript (u) {
  var o = document.createElement("script");
	o.src = u;
	o.type = "text/javascript";
	document.getElementsByTagName("head")[0].appendChild(o);
}

  function toURLvar (v) {
   return encodeURI((""+v).split("\\").join("\\\\").split("/").join("\\-")).split("#").join("%23");
  }

var elem = document.createElement('canvas');
var cis = !!(elem.getContext && elem.getContext('2d'));





var ldbtns = [];

function getBtnByProperty (prop, targ) {
  var o;
  for (var i = ldbtns.length-1; i>=0; i--) {
    o = ldbtns[i];
    if (o[prop] == targ) {
      return o;
    }
  }
}

var btrdo = {};
btrdo.rd_iv = -1;
btrdo.btns = [];

function btnRedrawLoop () {
  for (var i = btrdo.btns.length-1; i>=0; i--) {
    var o = btrdo.btns[i];
    o.rdto ++;
    //ctx.clearRect(0, 0, canvas.width, canvas.height);
    var av = .32;
    if (o.rdto >= 100) {
      av = 1;
      btrdo.btns.splice(i, 1);
      o.redrawing = false;
     }
    o.internalRedraw(av);
  }
  if (btrdo.btns.length == 0) {
    clearInterval(btrdo.rd_iv);
    btrdo.rd_iv = -1;
  }
 }

function makeBtn (pdiv, u, ww, hh, cmx, cmy, cmw, cmh) {
  var div = document.createElement('div');
  div.style.position = 'absolute';
  div.style.left = cmx+'px';
  div.style.top = cmy+'px';
  div.style.width = cmw+'px';
  div.style.height = cmh+'px';
  if (cis) {
    div.style.background = 'rgba(0, 0, 0, 0)';
    div.style.opacity = '0.0';
  }
  
  var ii = new Image();
  var o = {};
  o.onclick = function () {};
  o.onload = function () {};
  o.u = u;
  o.div = div;
  o.ww = ww;
  o.hh = hh;
  o.cmx = cmx;
  o.cmy = cmy;
  o.cmw = cmw;
  o.cmh = cmh;
  o.ii = ii;
  o.redrawing = false;
  /*
  o.ppdiv = ppdiv;
  var pdiv = document.createElement('div');
  ppdiv.appendChild(pdiv);
  */
  $(pdiv).css({position: 'relative', width: ww+'px', height: hh+'px', 'margin-left': 'auto', 'margin-right': 'auto', opacity: 0});
  o.pdiv = pdiv;
  o.pda = 0;
  
  if (cis) {
    var canvas,ctx;
    canvas = document.createElement('canvas');
    canvas.width = ww;
    canvas.height = hh;
    ctx = canvas.getContext('2d');
    o.canvas = canvas;
    $(canvas).css({position: 'absolute', left: '0px', top: '0px'});
    o.ctx = ctx;
    
    var buf_canvas,buf_ctx;
    buf_canvas = document.createElement('canvas');
    buf_canvas.width = ww;
    buf_canvas.height = hh;
    buf_ctx = buf_canvas.getContext('2d');
    o.buf_canvas = buf_canvas;
    o.buf_ctx = buf_ctx;
    pdiv.appendChild(canvas);
  }
  pdiv.appendChild(div);
  o.div = div;
  ldbtns.push(o);
  ii.onload = function () {
    var o = getBtnByProperty('ii', this);
    var u = o.u;
    var div = o.div;
    var pdiv = o.pdiv;
    var canvas = o.canvas;
    if (cis) {
     o.ctx.globalAlpha = 1;
     o.ctx.clearRect(0, 0, canvas.width, canvas.height);
     //o.buf_ctx.drawImage(o.ii, 0, 3 * o.hh, o.ww, o.hh, 0, 0, o.ww, o.hh);
     o.buf_ctx.drawImage(o.ii, 0, 0, o.ww, o.hh, 0, 0, o.ww, o.hh);
     o.ctx.drawImage(o.buf_canvas, 0, 0);
    } else {
     pdiv.style.background = 'url("'+u+'")';
    }
    pdiv.style.width = o.ww+'px';
    pdiv.style.height = o.hh+'px';
    pdiv.style.backgroundPosition = '0px 0px';
    div.style.cursor = 'pointer';
    o.mo = false;
    o.md = false;
    o.dis = false;
    o.bsha = 0;
    o.redraw = function () {
     o.rdto = 0;
     o.addToRedrawArray();
    }
   
   o.addToRedrawArray = function () {
     if (btrdo.rd_iv == -1) {
      btrdo.rd_iv = setInterval('btnRedrawLoop()', 35);
     }
     if (!this.redrawing) {
      this.redrawing = true;
      btrdo.btns.push(this); 
     }
   }
   
   o.internalRedraw = function (av) {
      var sy = 0;
      if (this.md) { 
        sy = (2 * this.hh);
      } else if (this.mo) {
        sy = this.hh;
      }
     if (cis) {
       var buf_canvas = this.buf_canvas;
       var buf_ctx = this.buf_ctx;
       buf_ctx.globalAlpha = av;
       buf_ctx.drawImage(this.ii, 0, sy, this.ww, this.hh, 0, 0, this.ww, this.hh);
       
       var canvas = this.canvas;
       var ctx = this.ctx;
       
       var dbsha;
       if ((!this.dis) && (this.mo)) {
        dbsha = 1;
       } else {
        dbsha = 0.2;
       }
       
       if (this.bsha < dbsha) {
        this.bsha += .05;
        if (this.bsha >= dbsha) {
         this.bsha = dbsha;
        }
       } else {
        this.bsha -= .05;
        if (this.bsha <= dbsha) {
         this.bsha = dbsha;
        }
       }
       
       ctx.globalAlpha = 1;
       ctx.clearRect(0, 0, canvas.width, canvas.height);
       
       ctx.globalAlpha = this.bsha;
       ctx.drawImage(this.ii, 0, 3 * this.hh, this.ww, this.hh, 0, 0, this.ww, this.hh);

       ctx.globalAlpha = 1;
       ctx.drawImage(buf_canvas, 0, 0);
     } else {
       this.pdiv.style.backgroundPosition = '0px '+((sy>0)?(-sy):0)+'px';
     }
     var dpda = this.dis?.25:1;
     this.pda += (dpda - this.pda) * .15;
     if (Math.abs(this.dpda - this.pda) < .1) {
      this.pda = this.dpda; 
     }
     this.pdiv.style.opacity = this.pda;
   }

    o.setEnabled = function (e) {
      o.dis = !e;
      o.redraw();
      if (e) { 
       o.div.style.cursor = 'pointer';
      } else {
       o.mo = false;
       o.md = false;
       o.pdiv.style.backgroundPosition = '0px 0px';
       o.div.style.cursor = 'auto';
      }
    }
    $(div).mouseover(function () {
      var o = getBtnByProperty('div', this);
      if (!o.dis) {
       o.mo = true;
       o.redraw();
      }
    });
    $(div).mouseout(function () {
      var o = getBtnByProperty('div', this);
      if (!o.dis) {
       o.mo = false;
       o.redraw();
       //o.pdiv.style.backgroundPosition = '0px 0px';
      }
    });
    $(div).mousedown(function () {
      var o = getBtnByProperty('div', this);
      if (!o.dis) {
       o.md = true;
       o.redraw();
       //o.pdiv.style.backgroundPosition = '0px -'+(2 * o.hh)+'px';
      }
    });
    $(div).mouseup(function () {
      var o = getBtnByProperty('div', this);
      if (!o.dis) {
       o.internalRedraw(1);
       o.md = false;
       o.redraw();
       //o.pdiv.style.backgroundPosition = '0px 0px'; 
      }
    });
    $(window).mouseup(function () {
      for (var i = ldbtns.length-1; i>=0; i--) {
        var o = ldbtns[i];
        if (o.md) {
         o.md = false;
         o.redraw();
        }
      }
    });
    $(div).click(function () {
      var o = getBtnByProperty('div', this);
      if (!o.dis) {
       o.onclick();
      }
    });
    o.redraw();
    o.onload();
  }
  ii.src = u;
  return o;
}

