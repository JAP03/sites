	var want_search_mtm = 0;
	var last_search_text = "";





	var mt_chc = 0;
	
	function searchLoop () {
	  var mtm = new Date().getTime();
	  want_search_mtm = mtm;
	  var t = ""+surtch.value;
	  var going = false;
	  if (t != last_search_text) {
	    var valid = false;
	    for (var i = t.length-1; i>=0; i--) {
	      var v = t.charCodeAt(i);
	      if (v > 0x20) {
	       valid = true;
	       break; 
	      }
	    }
	    if (!valid) {
	      t = "";
	    }
	    last_search_text = t;
		mt_chc = 0;
		going = true;
	  }
	  if (mt_chc >= 5) {
		t = "";
		going = true;
	  }
	  if (going) {
		t = t.trim();
		var wrp = false;
	    for (var i = memes.length-1; i>=0; i--) {
			var o = memes[i];
			o.lit = false;
			if (o.ln == t) {
				o.lit = true;
				wrp = true;
				break;
			}
		}
		if (!wrp) {
		    for (var i = memes.length-1; i>=0; i--) {
				var o = memes[i];
				o.lit = false;
				if (t.length > 0) {
					var ln = o.ln;
					if (ln.indexOf(t) >= 0) {
						o.lit = true;
						wrp = true;
					}
				}
			}
		}
		var any_lit = false;
	    for (var i = memes.length-1; i>=0; i--) {
			var o = memes[i];
			if (o.lit) {
				any_lit = true;
				break;
			}
		}
		if (!any_lit) {
			for (var i = memes.length-1; i>=0; i--) {
				var o = memes[i];
				if (o.has_big_btn) {
					if (!o.big_btn.div_shown) {
						o.big_btn.div_shown = true;
						$(o.big_btn.div).show();
					}
				}
			}
		} else {
			for (var i = memes.length-1; i>=0; i--) {
				var o = memes[i];
				if (o.has_big_btn) {
					if (o.lit && (!o.big_btn.div_shown)) {
						o.big_btn.div_shown = true;
						$(o.big_btn.div).show();
					} else if ((!o.lit) && o.big_btn.div_shown) {
						o.big_btn.div_shown = false;
						$(o.big_btn.div).hide();
					}
				}
			}
		}
		if (wrp) {
			repaintMemeTypes(false);
		}
	  }
	}


	var searchloop_iv = -1;

	surtch.onfocus = function () {
	 if (searchloop_iv == -1) {
	  searchloop_iv = setInterval('searchLoop()', 100);
	 } 
	}

	surtch.onblur = function () {
	 if (searchloop_iv != -1) {
	   clearInterval(searchloop_iv);
	   searchloop_iv = -1;
	 } 
	}
	

	</script>


    
      
