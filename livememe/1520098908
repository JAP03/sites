			 o.canvas.style.display = 'inline';
			}
			for (var j = i; j<dinks.length; j++) {
			 o = dinks[j];
			 o.mrdiv.style.display = 'none';
			 o.canvas.style.display = 'none';
			}
			break;
		   }
		   fw = o.x + o.width;
		  }
		 }
		
		 dinkdiv.style.left = Math.floor((wv-fw)/2)+'px';
		}
		

	    $(window).resize(function () {
	     if (doc_ready) {
	      resizeDinks();
	     }
	    });

		
	}
}

</script>



