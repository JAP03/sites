  $(window).resize(function(){
	//jCarouselResize();
	
	var $this = $( "#autoscroll-carousel" );
	var parent = $this.parent();
        if(parent.length) {
            var parentClass = parent.attr('class').split(' ').join('.');
            var containerWidth = parent.width();
            var containerHeight = parent.height();
            var top = (containerHeight - $this.height()) / 2;
            var width = containerWidth * 0.9;
            $this.css({'top': top, 'width': width});
            $('head').append('<style>.' + parentClass + ':before{left:' + (containerWidth * 0.05 - 3) + 'px;}</style>');
            $('head').append('<style>.' + parentClass + ':after{right:' + (containerWidth * 0.05 - 3) + 'px;}</style>');
        }
    });
  
