  $(document).ready(function(){
    $('.whatsnew-outer .item').mouseenter(function(){
        $(this).children('.rel').stop().animate({"top": "-144px"}, "slow");
      }).mouseleave(function(){
        $(this).children('.rel').stop().animate({"top": "0px"}, "fast");
      });
  });
  
  $(document).ready(function(){
    var fixadent = $("#zone-menu-wrapper");
    var pos = fixadent.offset();
    $(window).scroll(function() { 
     if($(this).scrollTop() > pos.top && fixadent.css('position') == 'static') {
         fixadent.addClass('fixed');
     } 
     else if($(this).scrollTop() <= pos.top && fixadent.hasClass('fixed')) {
         fixadent.removeClass('fixed');
     }
   })
  });
  
    $(document).ready(function() { 
        $(".fcgreen").click(function() {
            $(".target").slideDown(function(){
                $('.target .container .close_login').click(function(){
                    $('.target').slideUp();
                });	
                $(document).bind('keydown', function(e) { 
                    if (e.which == 27) {
                        $('.target').slideUp();
                    }
                });
            });
            return false;
        });
    });

