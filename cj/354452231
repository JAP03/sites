			$('#pub_why_menu li:eq('+ix+') a').addClass('active-trail');
		},
		btnPrevCallback: function(){
			ix = $('#pub_why_slider').roundabout('getChildInFocus');
			$('#pub_why_menu li').removeClass('active-trail');
			$('#pub_why_menu li a').removeClass('active-trail');
			
			$('#pub_why_menu li:eq('+ix+')').addClass('active-trail');
			$('#pub_why_menu li:eq('+ix+') a').addClass('active-trail');
		}
		
		
	}, function() {
		$(this).fadeTo(500, 1);
	});
	
	$('#pub_why_menu li a').click(function(){
		ix = $(this).parent().data('ix');
		$('#pub_why_slider').roundabout('animateToChild', ix);
			$('#pub_why_menu li').removeClass('active-trail');
			$('#pub_why_menu li a').removeClass('active-trail');
			
			$(this).addClass('active-trail');
			$(this).parent().addClass('active-trail');

		return false;
	});


	$('#pub_why_menu li a:first').trigger('click');
	
	
	
	



	
   
