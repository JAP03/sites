  $(document).ready(function(){
    $('.cj-search .search-by-page-form .form-submit').attr('value', '');
    $('.cj-search .search-by-page-form .form-item-keys input').attr('placeholder', 'Site Search');
    
    $('.cj-search-blog .search-by-page-form .form-submit').attr('value', '');
    $('.cj-search-blog .search-by-page-form .form-item-keys input').attr('placeholder', 'Search Blog...');
    
    $('input[placeholder], textarea[placeholder]').placeholder();
  });
  
