  $(document).ready(function(){

	$( "#autoscroll-carousel" ).imageScroller( {loading:'Wait please...', speed: 10000} );
  
	$('.front #block-block-2 .content ul li img').each(function(ix, elem) {
		//console.log($(elem).attr('src'));
		imgsrc = $(elem).attr('src');
		imgw = $(elem).width();
		imgh = $(elem).height();
		//console.log($(elem).attr('src'));
		$(elem).before('<span class="imgreplacer" style="margin:0 auto; display:block; background: url('+imgsrc+') 50% top no-repeat; width: '+imgw+'px; height: '+imgh+'px;">&nbsp;</span>').remove();
	});
	
	//jCarouselResize();
	//jCarouselInit();
	

/*ADVERTISER CAROUSEL*/	
	
	
	$('#adv_why_slider').roundabout({
		tilt: 0.6,
		minScale: 0.6,
		minOpacity: 1,
		duration: 400,
		easing: 'easeOutQuad',
		dropEasing: 'easeOutBounce',
		btnNext: '#adv_slider_next',
		btnPrev: '#adv_slider_prev',
		btnNextCallback: function(){
			ix = $('#adv_why_slider').roundabout('getChildInFocus');
			$('#adv_why_menu li').removeClass('active-trail');
			$('#adv_why_menu li a').removeClass('active-trail');
			
			$('#adv_why_menu li:eq('+ix+')').addClass('active-trail');
			$('#adv_why_menu li:eq('+ix+') a').addClass('active-trail');
		},
		btnPrevCallback: function(){
			ix = $('#adv_why_slider').roundabout('getChildInFocus');
			$('#adv_why_menu li').removeClass('active-trail');
			$('#adv_why_menu li a').removeClass('active-trail');
			
			$('#adv_why_menu li:eq('+ix+')').addClass('active-trail');
			$('#adv_why_menu li:eq('+ix+') a').addClass('active-trail');
		}
		
		
	}, function() {
		$(this).fadeTo(500, 1);
	});
	
	$('#adv_why_menu li a').click(function(){
		ix = $(this).parent().data('ix');
		$('#adv_why_slider').roundabout('animateToChild', ix);
			$('#adv_why_menu li').removeClass('active-trail');
			$('#adv_why_menu li a').removeClass('active-trail');
			
			$(this).addClass('active-trail');
			$(this).parent().addClass('active-trail');

		return false;
	});
	
	$('#adv_why_menu li a:first').trigger('click');
	
	$('#adv-tbl-service tbody tr:odd').addClass('odd');
	$('#adv-tbl-service tbody tr:even').addClass('even');

	

/* PUBLISHER CAROUSEL */
	$('#pub_why_slider').roundabout({
		tilt: 0.6,
		minScale: 0.6,
		minOpacity: 1,
		duration: 400,
		easing: 'easeOutQuad',
		dropEasing: 'easeOutBounce',
		btnNext: '#pub_slider_next',
		btnPrev: '#pub_slider_prev',
