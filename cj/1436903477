docCookies = {
    set: function (sKey, sValue, vEnd, sPath, sDomain) {
        if (!sKey || /^(?:expires|max\-age|path|domain|secure)$/.test(sKey)) { return; }
        var sExpires = "", d = new Date();
        if (typeof vEnd == "number") {
            d.setTime(d.getTime() + (vEnd * 1000));
            sExpires = "; max-age=" + vEnd + "; expires=" + d.toGMTString();
        }
        document.cookie = escape(sKey) + "=" + escape(sValue) + sExpires + (sDomain ? "; domain=" + sDomain : "") + (sPath ? "; path=" + sPath : "");
    },
    has: function (sKey) { return (new RegExp("(?:^|;\\s*)" + escape(sKey).replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=")).test(document.cookie); },
    enabled: function () {
        if (navigator.cookieEnabled) { return true; }
        this.set('cookietest', 1);
        var ret = this.has('cookietest');
        this.set('cookietest', 1, -999);
        return ret;
    }
};


(function($) {
  $(function() {

    var re = /\.cj\.com$/;
    if (re.exec(document.domain) && docCookies.enabled() && !docCookies.has('lang_r')) {
        /* set it the first time in case it doesnt work... */
        docCookies.set('lang_r', '0', 3600, '/', '.cj.com');
        $.ajax({
            url: 'http://www.cj.com/_lang_r/parse.php',
            dataType: 'script',
            data: 'r=' + Math.floor(Math.random() * 1000),
            success: function () {
                /* Set a longer cookie so they don't get reprompted */
                docCookies.set('lang_r', '0', 604800, '/', '.cj.com');
            }
        });
    }

});
})(jQuery);
