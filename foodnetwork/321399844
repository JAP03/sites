<!-- CONTROL SCRIPTS FIRST LOOK COOKIE -->
<script>


function Set_firstlook(cookieName,cookieValue) {
var nowAdops = new Date();
var expireDateAdops = new Date(nowAdops.getFullYear(), nowAdops.getMonth(), nowAdops.getDate(), 23, 59, 59, 999);
document.cookie = cookieName+"="+escape(cookieValue) + ";expires="+expireDateAdops.toGMTString()+";path=/";
}

Set_firstlook('userType', 'firstlook');
</script>