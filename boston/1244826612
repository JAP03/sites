$(function() {

	var whichSlide= $("div.doThings").scrollable({
		api:true,
		speed:0,
		size: 1,
		keyboard:false,
		loop: true
	});
	
	var totalPages = whichSlide.getPageAmount();
	var randomPageIndex=Math.floor(Math.random()*totalPages);
	var firstTime =true;
	
	whichSlide.onSeek(function() {
		if(firstTime){
			this.getConf().speed = 400;
			firstTime=false;
		}
	});
	
	whichSlide.setPage(randomPageIndex);
	sliiide = whichSlide;
});