/*global $, jQuery, document*/
(function ($) {
    "use strict";
    $(document).ready(function () {
        jQuery.each(jQuery('#regmod').find('input.field'), function (index,el) {
            jQuery(el).val('');
            jQuery(el).click(function (e) {
                jQuery('#' + e.currentTarget.id + 'Label').css('display', 'none');
                jQuery('#' + e.currentTarget.id).focus();
            });
            jQuery(el).blur(function (e) {
                if (jQuery('#' + e.currentTarget.id).val() == '') {
                    jQuery('#' + e.currentTarget.id + 'Label').css('display', 'block');
                }
            });
            jQuery(el).keydown(function (e) {
                var TABKEY = 9;
                if(e.keyCode == TABKEY) {
                	jQuery('#' + e.currentTarget.id + 'Label').next().next().css('display','none')
                    jQuery('#' + e.currentTarget.id).next().focus();
                }
            });
        });
        jQuery('#chooseScreen').after('<label id="chooseScreenLabel" class="overTxtLabel choose-screen" for="chooseScreen">Choose a Screen Name</label>');
        jQuery('#choosePassword').after('<label id="choosePasswordLabel" class="overTxtLabel choose-pass" for="choosePassword">Choose a password</label>');
        jQuery('#confirmPassword').after('<label id="confirmPasswordLabel" class="overTxtLabel confirm-pass" for="confirmPassword">Re-enter your password</label>');
    });
}(jQuery));