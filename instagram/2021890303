24*7)<1){var days=Math.round(seconds/(60*60*24));if(shorten){return sprintf(i18n("%(days)sd"),{days:days})}else{if(days==1){return i18n("1 day ago")}else{return sprintf(i18n("%(days)s days ago"),{days:days})}}}else if(seconds/(60*60*24*30)<1){var weeks=Math.round(seconds/(60*60*24*7));if(shorten){return sprintf(i18n("%(weeks)sw"),{weeks:weeks})}else{if(weeks==1){return i18n("1 week ago")}else{return sprintf(i18n("%(weeks)s weeks ago"),{weeks:weeks})}}}else{var weeks=Math.round(seconds/(60*60*24*7));var months=Math.round(seconds/(60*60*24*30));if(shorten){return sprintf(i18n("%(weeks)sw"),{weeks:weeks})}else{if(months==1){return i18n("1 month ago")}else{return sprintf(i18n("%(months)s months ago"),{months:months})}}}};exports.ago=function(timestamp){return _ago(timestamp,false)};exports.agoShortened=function(timestamp){return _ago(timestamp,true)}});window.commonjsBundler.evaluator.define("lib/ui/common/Timestamp_css",function(require,exports,module){require("infra/packager").injectCSS("lib/ui/common/Timestamp",".timestamp {\n  color: #aaaaaa;\n  font-weight: bold; }\n","")});window.commonjsBundler.evaluator.define("lib/ui/common/Wrapper",function(require,exports,module){module.exports={}});window.commonjsBundler.evaluator.define("lib/ui/common/Wrapper_css",function(require,exports,module){require("infra/packager").injectCSS("lib/ui/common/Wrapper",'@charset "UTF-8";\n/* (c) 2012 Instagram, Inc, */\n/* @license\n * MyFonts Webfont Build ID 2164953, 2012-03-23T23:06:30-0400\n *\n * The fonts listed in this notice are subject to the End User License\n * Agreement(s) entered into by the website owner. All other parties are\n * explicitly restricted from using the Licensed Webfonts(s).\n *\n * You may obtain a valid license at the URLs below.\n *\n *\n * Webfont: Proxima Nova Regular by Mark Simonson\n * URL: http://www.myfonts.com/fonts/marksimonson/proxima-nova/regular/\n * Licensed pageviews: unlimited\n *\n * Webfont: Proxima Nova Semibold by Mark Simonson\n * URL: h