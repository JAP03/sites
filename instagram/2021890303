(){var video=VideoJS({skipDefaultSkin:true,playing:this.state.playing,muted:this.state.muted,onPlayerEvent:this.handlePlayerEvent,preload:"none",controls:this.props.controls,hasSound:this.props.hasSound,src:this.props.src,type:this.props.type,poster:this.props.poster,width:"100%",height:"100%"});return this.transferPropsTo(React.DOM.div({className:cx({Video:true,vOffsetIndicator:this.props.offsetIndicator,vStatesHide:!this.props.embed&&!this.state.forceShow&&!this.state.mouseOver}),onMouseEnter:this.handleMouseEnter,onMouseLeave:this.handleMouseLeave},video,React.DOM.a({href:"javascript:;",role:"button",className:cx({vCoverLayer:true}),onClick:this.handlePlay}),React.DOM.div({className:cx({vStates:true})},React.DOM.a({href:"javascript:;",role:"button",className:cx({vStatePlay:!this.state.playing,vStatePause:!this.props.offsetIndicator&&this.state.playing,vStatePaused:this.props.offsetIndicator&&this.state.playerState===PLAYER_STATES.paused}),onClick:this.handlePlay}),React.DOM.a({href:"javascript:;",role:"button",className:cx({vStateSound:!this.state.muted&&this.props.hasSound,vStateMuted:this.state.muted||!this.props.hasSound}),onClick:this.handleMute}))))}});module.exports=Video});window.commonjsBundler.evaluator.define("lib/io/logClientAction",function(require,exports,module){var emptyFunction=require("emptyFunction");var ExecutionEnvironment=require("ExecutionEnvironment");var QUEUE_TIMEOUT=1e4;var queue=[];function logKeys(keys,cb){if(keys.length>0){$.post("/client_action/",{k:keys.join(",")},cb)}}function dispatchQueue(){logKeys(queue,emptyFunction);queue.length=0}function logClientAction(key,cb){if(cb){logKeys([key],cb)}else{queue.push(key)}}if(ExecutionEnvironment.canUseDOM){window.setInterval(dispatchQueue,QUEUE_TIMEOUT)}module.exports=logClientAction});window.commonjsBundler.evaluator.define("lib/ui/media/Video_css",function(require,exports,module){require("infra/packager").injectCSS("lib/ui/media/Video",".Video {\n  position: relative; }\n\n.vStatesHide .