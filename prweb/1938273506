$(function() {
	
		// carousel //
		
		// generate auto links //
		
		$('#fn-controls').append('<ul id="carousel-controls">');
		
		var carouselElCounter = 0;
		var btnGoArray = new Array();
		
		$('ul#releases-carousel li').each( function(el) { 
			
			var theId = 'carousel-item-' + carouselElCounter;
			
			var li = $(document.createElement('li')).attr( 'id', theId );
			var a = $(document.createElement('a')).attr( 'href', '#' );
			$(li).append(a)
			$('#carousel-controls').append(li);
		
			btnGoArray.push('#' + theId);
			carouselElCounter++;
		
		});		
		
		
		$("#featured-news-wrap").jCarouselLite({
			btnNext: "#carousel-next",
			btnPrev: "#carousel-prev",
			visible: 1,
			speed: 300,
			auto: 2000,
			autoStop: 5,
			scroll: 1,
			btnGo: btnGoArray,
			afterEnd: function(el) {
				$('#carousel-controls li').removeClass('current'); 
				var index = $('#releases-carousel li').index(el);
				if ( index == 0 ) {
					index = carouselElCounter - 1;
				} else {
					if ( index > carouselElCounter ) {
						index = 0;
					} else {
						index = index - 1;
					}
				}
				$('#carousel-controls li:eq(' + index + ')').addClass('current'); 
			}
		});
		
		$('#carousel-controls li:eq(0)').addClass('current');
		
		// if we want to do auto-rotate //
		$('#carousel-next, #carousel-prev, #carousel-controls li').click(function() {
			//
		});


	});