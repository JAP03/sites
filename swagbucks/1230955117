<script type="text/javascript">
	if(/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ) {
		$('html').addClass('isMobile');
	}
	if($('#sbFooterWrap').is(':hidden')) {
