<script type="text/javascript">
	(function(){
		var $topBlock = $('#topBloc');
		var topbarHeight = $('#sbGlobalNav').height();
		var saveTopBlockHeight = $topBlock.height();
		var saveTopblockOuterHeight =  $topBlock.outerHeight(true);
		function setFooterBelowFold() {
			var topBlockXtraHeight = $topBlock.outerHeight(true) - $topBlock.height();
			var winHeight = $(window).height();
			if(winHeight > (saveTopblockOuterHeight + topbarHeight)) {
				$topBlock.height( (winHeight-topBlockXtraHeight-topbarHeight) + 10);
			}else {
				$topBlock.height(saveTopBlockHeight + (saveTopblockOuterHeight - saveTopBlockHeight));
			}
		}
		if(!/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {
			setFooterBelowFold();
			$(window).resize(setFooterBelowFold);
		}
	})();
	
</script>
	
