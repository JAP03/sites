Function.prototype.bind = function (element) {
	var that = this;
	return function () {
		that.call(element);
	}
};

var phantomPopunder = phantomPopunder || {};

phantomPopunder = {
	reRuntime        : phantomPopunder.reRuntime || 24,
	url              : phantomPopunder.url || "http://creatives.livejasmin.com/pu/w2/index.php",
	width            : phantomPopunder.width || 1100,
	height           : phantomPopunder.height || 800,
	top              : phantomPopunder.top || 0,
	left             : phantomPopunder.left || 0,
	cookieName       : phantomPopunder.cookieName || 'lj_popunder',
	tabDisabled      : phantomPopunder.tabDisabled || false,
	mainWindow       : top != self && typeof top.document.location.toString() == "string" ? top : self,
	popUp            : null,
	toleratedDomains : (function (toleratedDomains) {
		return "(" + toleratedDomains.join("|") + ")";
	}).call(this,
