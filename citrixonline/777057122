(function(jQuery) {
	jQuery.fn.latestPost = function(){
		var url = "http://news.citrixonline.com/wp-content/themes/twentyten/show-latest-post.php?callback=?";
		//Use jQuery getJSON method to fetch the data from the url and then create our unordered list with the relevant data.
		jQuery.getJSON(url, function(json){
		  		jQuery("span#latestPost").html(json.post);
		});
	};
})(jQuery);