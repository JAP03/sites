var utils = {};
utils.cookie = {
    /**
     * Utility for writing cookies. By default this method will ensure that both the name and value
     * of the cookie are URL encoded.
     *
     * @param name   the cookie's name.
     * @param value  the cookie's value.
     * @param expiry the expiration time in seconds.
     */
    write:function (name, value, expiry, encode) {
        var expirationDate, expires = '', domain = '';

        encode = typeof encode !== 'undefined' ? encode : true;

        if (!utils.common.isEmpty(expiry) && typeof(expiry) === 'number' && expiry !== 0) {
            expirationDate = new Date();
            expirationDate.setTime(expirationDate.getTime() + (expiry * 1000));
            expires = "; expires=" + expirationDate.toGMTString();
        } else {
            expires = "";
        }
        if (window.location.hostname != "localhost") {
            var domainParts, domainValue;
            domainParts = window.location.hostname.split(/:\/\/|\./);
            domainValue = domainParts.slice(-2).join('.');
            domain = "; domain=." + domainValue;
        } else {
            domain = "";
        }

        if (encode) {
            value = encodeURIComponent(value);
        }

        document.cookie = encodeURIComponent(name) + "=" + value + expires + "; path=/" + domain;
    },

    /**
     * Utility for reading cookies.
     *
     * @param name the name of the cookie to be read.
     * @param search whether or not we should search for a cookie which starts with the name given
     * @return {String} the cookie's value OR empty string if the cookie couldn't be found.
     */
    read:function (name, search, decode) {
        var cookieParts = document.cookie.split(';'), cookiePart, cookieValue, pattern, matches;

        decode = typeof decode !== 'undefined' ? decode : true;

        for (var i = 0; i < cookieParts.length; i++) {
            cookiePart = cookieParts[i];
            cookiePart = cookiePart.trimLeft();

            if (decode) {
                cookiePart = decodeURIComponent(cookiePart);
            }

            pattern = new RegExp(name + '=' + '(.+)');

            if (search) {
                pattern = new RegExp(name + '\\w+=' + '(.+)');
            }

            matches = cookiePart.match(pattern);

            if (matches) {
                cookieValue = matches[1];
                return cookieValue;
            }
        }

        return '';
    },

    /**
     * Utility for removing/deleting cookies.
     *
     * @param name the name of the cookie to be read.
     */
    remove:function (name) {
        utils.cookie.write(name, '', -1);
    }
};
utils.common = {
    /**
     * Utility function for determine object emptiness (i.e. null, undefined, or empty string).
     *
     * @param object the object to test for emptiness.
     * @return {boolean} true if the object is empty, false otherwise.
     */
    isEmpty:function (object) {
        return (object === null || object === undefined || (typeof(object) === "string" && object.trim() === '') ||
            (object.hasOwnProperty("length") && object.length === 0) );
    }
};

function getGeneralErrorResponse() {
    return {
        version:"2.0",
        status:{
            state:"failed",
            timestamp:(new Date()).toString(),
            errors:[
                {
                    code:"500",
                    applicationCode:"general.server.error",
                    message:"Server error. Unable to process your request due to a technical problem."
                }
            ]
        }
    };
}

function isMobile() {
    return isMobileDevice = navigator.userAgent.match(/ipad|iphone|ipod|android|blackberry|webos/i) != null
        || screen.width <= 480 || mobileCheck();
}


