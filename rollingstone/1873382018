$(document).ready(function(){
	// full page width carousel with 6 visible elements
	$('.simpleRibbonCarousel.scroll6').each(function(){
		if($(this).find('li').length > 6){
			$(this).jcarousel({ scroll: 6, buttonNextCallback: RS.showNextCarouselItem });
		}	
	});
	// Best List style carousel that shows 1 featurebox style element at a time
	$(".rsList").each(function(){
		if($(this).find('li').length > 1){
			$(this).jcarousel({
				scroll: 1,
				buttonNextCallback: RS.showNextCarouselItem
			});
		}
	});
	// two column wide carousel, shows 2 items at a time 2colFeatureBoxCarousel
	$(".twoColFeatureBoxCarousel,.twoColFullWidth").each(function(){
		if($(this).find('li').length > 2){
			$(this).jcarousel({
				scroll: 2,
				buttonNextCallback: RS.showNextCarouselItem
			});
		}
	});
	// full width carousel, 3 items, scrolls 1 each
	$(".threeColFullWidth").each(function(){
		if($(this).find('li').length > 3){
			$(this).jcarousel({
				scroll: 1,
				buttonNextCallback: RS.showNextCarouselItem
			});
		}
	});	

});
