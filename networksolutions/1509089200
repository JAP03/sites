	<script type="text/javascript">
   		var products = ";";
   		var productsArray = products.split(',');
   		
		var dotomiProductItems = "";
		
		for(var i = 0; i < productsArray.length; i++)
		{
			//change format  ;ACQ-DOM_COM-5.0-Y;1;64.95,;ACQ-DOM_NET-5.0-Y;1;64.95
			//to format		ACQ-DOM_COM-5.0-Y;64.95|ACQ-DOM_NET-5.0-Y;64.95
			var tmpString = productsArray[i];
			var tmpProdData = tmpString.split(';');
			if(tmpProdData.length == 4)
			{
				dotomiProductItems = dotomiProductItems + tmpProdData[1] + ";" + tmpProdData[3];
				if (i<productsArray.length-1)
				{
					dotomiProductItems = dotomiProductItems + "|";
				}				
			}	
		}		
	</script>
		
    	
	
	
	
	
	
	
	
	
	
	
		
			 
		
		
	
	
	
	
    
    	
    
     
    	
    	
    	 	
        		
    	  		
    	   	 		
    	  		
    	  	
      	
    
	
	
	
	
    	
    	
      		
    	
    
	
	
	 
	
	
	
	
	
	
	
	
		
	
	
	
		
		
			
		
	
	
	
	


	
	
	
		
