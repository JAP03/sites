var replaceJQuery = false;
	if(typeof window.jQuery != "undefined" && parseFloat(window.jQuery.fn.jquery) <= 1.8) {
		var replaceJQuery = true;
	}

if(!window.jQuery || replaceJQuery) {
	var script = document.createElement("script");
	script.src = "//cdnjs.cloudflare.com/ajax/libs/jquery/1.9.1/jquery.min.js";
	script.type = 'text/javascript';
	document.getElementsByTagName("head")[0].appendChild(script);
}

var checkReady = function(callback) { if (window.jQuery && parseFloat(window.jQuery.fn.jquery) > 1.8) { callback(jQuery); } else { window.setTimeout(function() { checkReady(callback); }, 100); } };

checkReady(function() {
	var script = document.createElement("script");
	script.src = "//cdn.cpxinteractive.com/slider/lib/js/jquery.easing.cookie.min.js";
	script.type = 'text/javascript';
	document.getElementsByTagName("head")[0].appendChild(script);
		
	var checkReady = function(callback) { if (window.jQuery.cookie) { callback(); } else { window.setTimeout(function() { checkReady(callback); }, 100); } };
	checkReady(function() {
		if(replaceJQuery) {
			window.jq = $.noConflict(true);
		} else {
			window.jq = $;
		}
		var script = document.createElement("script");
		
		script.src = "//cdn.cpxinteractive.com/slider/lib/js/app." + sliderOptions.version + ".js";
			
		// check for any params on this files src & pass em along
		var queryString = thisNode.src.replace(/^[^\?]+\??/,'');
		queryParams = queryString.split("&");
		if(queryString) {
			script.src += "?" + queryParams.join("&");
		}
			
		script.type = 'text/javascript';
		jq("body").append(script);
	});
});