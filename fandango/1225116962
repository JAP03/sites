function makePixels(fv)
{
  var segments = ['77247|720','77248|1440','40608|2160'];
  var conversion = '';
   
  if ( fv != undefined && fv != null && fv == 1) {
      segments.push('20062|336');
      segments.push('20063|720'); 
  }
  
  
  //Get meta tag content
  var genres = '';
  var ratings = '';
  var mG = document.getElementsByTagName("meta");
  for(var i=0; i<mG.length; i++){
   if(mG[i].getAttribute("itemprop")=="genre"){
    genres = mG[i].content;
   }
   if(mG[i].getAttribute("itemprop")=="contentRating"){
    ratings = mG[i].content;
   }
  } 
  
  //Conversion Pixel -- find movie title on conversion page
  var movieTitle = '';
  var pD = document.getElementsByTagName("h4");
  for(var i=0; i<pD.length; i++){
   if(pD[i].className=="movieTitle"){
    movieTitle = pD[i].innerHTML;
   }
  }

  //Number of tickets purchased
  var totalTics = 0;
  var childTics = 0;
  var tD = document.getElementsByTagName('td');
  for(var i=0; i<tD.length; i++){
   if(tD[i].className=="quantity"){
		var numTics = tD[i].innerHTML;
		nlTics = numTics.split(" ");
		var str = nlTics.join();
		if(sVar(str,'adult') || sVar(str,'senior')){
			var ta = str.split(",");			
			totalTics += parseInt(ta[0]);
		}else if(sVar(str,'child')){
			var ta = str.split(",");			
			totalTics += parseInt(ta[0]);
			childTics += parseInt(ta[0]);
		}
    }
   }

   if(totalTics > 0){
     if(totalTics == 1){
       segments.push('79025|1440');
     }else if(totalTics == 2){
       segments.push('79026|1440');
     }else if(totalTics >= 3){
       segments.push('79027|1440');
     }
     if(childTics == 1){
       segments.push('79028|1440');
     }else if(childTics == 2){
       segments.push('79029|1440');
     }else if(childTics >= 3){
       segments.push('79030|1440');
     }
   }

  
//==========================================
