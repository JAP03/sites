  }else if(sHref('47ronin') || sHref('144848')){
    segments.push('77839|1440');
	conversion = 'http://trk.tidaltv.com/ILogger.aspx?Event=Action&apid=2895';
  }else if(((movieTitle == '47 Ronin 3D <span class="movieRating">(PG-13)</span>') || (movieTitle == '47 Ronin <span class="movieRating">(PG-13)</span>')) && sHref('ticketorderconfirmationpage')){
	conversion = 'https://trk.tidaltv.com/ILogger.aspx?Event=Action&apid=2897';
  }else if(sHref('47roninmovie.com')){
	conversion = 'http://trk.tidaltv.com/ILogger.aspx?Event=Action&apid=2894';
	segments.push('82146|1440');
