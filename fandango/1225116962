    segments.push('45926|1440');
  }
 
 //END MOVIE GENRES

  var img = document.createElement("img");
  img.src = "https://set.tidaltv.com/Ping.aspx?tids="+segments.join();
  img.height=1;
  img.width=1;
  
  if(conversion != ""){
    var img2 = document.createElement("img");
    img2.src = conversion;
    img2.height=1;
    img2.width=1;
  }
 }

function sHref(term){
  if(window.location.href.toLowerCase().indexOf(term) > -1){
    return 1;	
  }else{
    return 0;
  }
}


function sVar(v,term){
  if(v.toLowerCase().indexOf(term) > -1){
    return 1;	
  }else{
    return 0;
  }
}


function vSC(n,v,d) {
  if (d) {
    var date = new Date();
    date.setTime(date.getTime()+(d*24*60*60*1000));
    var e = "; expires="+date.toGMTString();
  }
  else var e = "";
  document.cookie = n+"="+v+e+"; path=/";
}

function vGC(n){
  var re = new RegExp(n + "=([^;]+)");
  var value = re.exec(document.cookie);
  return (value != null) ? unescape(value[1]) : 0;
}

if(vGC("ffv30") == 1 && vGC("ffvS") == 0){
  makePixels(1);
}else{
  makePixels(0);
}
vSC("ffv30", "1",30);
vSC("ffvS", "1");