	}

 
//END CUSTOM PIXELS
//==========================================
//FANDANGO SITE NAV
  //Showtime Searcher
  if(sHref('movietimes')){
    segments.push('53801|1440');
  }
  //End Showtime Searcher
  
  //Ticket Buyer
  if(sHref('ticketorderconfirmationpage')){
    segments.push('40658|1440');
  }
  //End Ticket Buyer
//END FANDANGO SITE NAV

//========================================== 

  //MOVIE RATINGS
  if(sVar(ratings,'pg-13')){
    segments.push('79022|1440');
  }else if(sVar(ratings,'pg')){
    segments.push('79021|1440');
  }else if(sVar(ratings,'g')){
    segments.push('79020|1440');
  }else if(sVar(ratings,'r')){
    segments.push('79023|1440');
  }else if(sVar(ratings,'nc-17')){
    segments.push('79024|1440');
  }

//END MOVIE RATINGS 

//==========================================  

//TICKET BUYER COUNTS

//END TICKET BUYER COUNTS


//==========================================  

//MOVIE GENRES
 
 //3D Movies 
  if(sHref('genrefilter=3d') || sVar(genres,'3d3d')){
    segments.push('45927|1440');
  }
  
 //Action & Adventure
  if(sHref('genrefilter=action%2fadventure') || sVar(genres,'action') || sVar(genres,'adventure')) {
    segments.push('45913|1440');
  }
 
 //Animated
  if(sHref('genrefilter=animated') || sVar(genres,'animated')) {
