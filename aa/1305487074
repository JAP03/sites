(function($){$pillboxes=$('[data-behavior~="pillbox"]');$pillboxes.addClass("pillbox");$pillboxes.find('input[type="radio"]').focus(function(e){$(this).parent('[data-behavior~="pillbox"]').addClass("js-focus");}).blur(function(e){$(this).parent('[data-behavior~="pillbox"]').removeClass("js-focus");});$pillboxes.click(function(e){$pillboxes.removeClass("selected");if($(this).find('input[type="radio"]:checked')){$(this).addClass("selected");}else{$(this).removeClass("selected");}});$pillboxes.each(function(i,item){if($(this).find('input[type="radio"]:checked').length>0){$(this).addClass("selected");}});})(jQuery);(function($){if($("html").hasClass("lt-ie8")){return;}$prettyControls=$('[data-behavior~="prettify"]');$prettyControls.addClass("prettify").prepend('<div class="prettyBox"></div>');$prettyControls.find('input[type="checkbox"]').focus(function(e){$(this).parent('[data-behavior~="prettify"]').addClass("js-focus");}).blur(function(e){$(this).parent('[data-behavior~="prettify"]').removeClass("js-focus");});$prettyControls.click(function(e){if($(this).find('input[type="checkbox"]').is(":checked")){$(this).addClass("selected");}else{$(this).removeClass("selected");}});$prettyControls.each(function(i,item){if($(this).find('input[type="checkbox"]:checked').length>0){$(this).addClass("selected");}});})(jQuery);