if(typeof jQuery=='undefined'){
	window.setTimeout(runTracker, 600);   //Run Tracker function after jQuery is loaded
}else{
	imp_tk();
}
function runTracker(){
	imp_tk();		
}  
function imp_tk(){
	$(document).ready(function(){
		var links_url_t_a = $('a[href^="http://tracking.smartclicksystem.com"]');
		var links_arr_t_l=new Array();        
			for (var i = 0; i < links_url_t_a.length; i++) {     
					links_arr_t_l.push($(links_url_t_a[i]).attr('href'));
			}
			
			var _data_info_t = {'details' : links_arr_t_l.join(',')}
			$.ajax({
					async:true,
					url: 'http://tracker.smartclicksystem.com/imp.php',
					type: 'GET',
					data:(_data_info_t),
					dataType: 'jsonp',
					cache:false
			});
	});
}